{
  "name": "jadob/jadob",
  "license": "MIT",
  "description": "PHP Framework.",
  "require": {
    "php": "^8.3",
    "twig/twig": ">= 3.0",
    "symfony/http-foundation": "^7.2",
    "doctrine/orm": ">= 3.3.2",
    "psr/container": "^1.0",
    "symfony/form": ">= 7.2",
    "symfony/validator": ">= 7.2",
    "symfony/twig-bridge": ">= 7.2",
    "symfony/security-csrf": ">= 7.2",
    "monolog/monolog": "^2.0",
    "pagerfanta/pagerfanta": "^2.0",
    "psr/event-dispatcher": "^1.0",
    "psr/log": "^1.1",
    "symfony/translation": ">= 7.2",
    "aws/aws-sdk-php": "^3.158",
    "ext-json": "*",
    "doctrine/cache": "^1.12",
    "doctrine/dbal": ">=2.13",
    "symfony/mime": "*",
    "symfony/cache": "^7.2",
    "marcj/topsort": "^2.0",
    "nunomaduro/termwind": "^2.3"
  },
  "require-dev": {
    "phpstan/phpstan-strict-rules": "^2.0",
    "phpstan/phpstan": "^2.1",
    "vimeo/psalm": "^6.10",
    "phpunit/phpunit": "^10.5",
    "friendsofphp/php-cs-fixer": "^3.75"
  },
  "autoload": {
    "psr-0": {
      "Jadob\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "Jadob\\": "tests/unit/"
    }
  },
  "replace": {
    "jadob/core": "self.version",
    "jadob/event-sourcing": "self.version",
    "jadob/config": "self.version",
    "jadob/router": "self.version",
    "jadob/bridge-doctrine-migrations": "self.version",
    "jadob/bridge-doctrine-dbal": "self.version",
    "jadob/bridge-doctrine-persistence": "self.version",
    "jadob/bridge-doctrine-orm": "self.version",
    "jadob/bridge-aws-ses": "self.version",
    "jadob/bridge-aws": "self.version",
    "jadob/bridge-monolog": "self.version",
    "jadob/bridge-symfony-console": "self.version",
    "jadob/bridge-symfony-form": "self.version",
    "jadob/bridge-symfony-translation": "self.version",
    "jadob/bridge-twig": "self.version",
    "jadob/container": "self.version",
    "jadob/contracts-webhook": "self.version",
    "jadob/dashboard": "self.version",
    "jadob/url": "self.version",
    "jadob/runtime": "self.version",
    "jadob/security-auth": "self.version",
    "jadob/security-supervisor": "self.version",
    "jadob/typed-aws": "self.version",
    "jadob/typed-aws-lambda": "self.version",
    "jadob/typed-geo": "self.version",
    "jadob/typed-telegram": "self.version",
    "jadob/feature-flag": "self.version",
    "jadob/event-dispatcher": "self.version",
    "jadob/contracts-dashboard": "self.version",
    "jadob/debug": "self.version",
    "jadob/webhook-handler": "self.version",
    "jadob/webhook-provider-telegram": "self.version",
    "jadob/objectable": "self.version"
  },
  "config": {
    "allow-plugins": {
      "infection/extension-installer": true
    }
  }
}
